{
  "name": "devconnector",
  "version": "1.0.0",
  "description": "Social network for delevopers",
  "main": "server.js",
  "scripts": {
    "build": "tsc -p .",
    "client": "cd client && yarn start",
    "server": "cd server && nodemon server.ts",
    "dev": "concurrently --kill-others \"yarn server\" \"yarn client\""
  },
  "keywords": [
    "MERN-stack",
    "MongoDB",
    "ExpressJS",
    "ReactJS",
    "NodeJS"
  ],
  "author": "Serafim Gavrilov",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "express-validator": "^6.6.0",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.22",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "0.0.36",
    "@types/express": "^4.17.7",
    "@types/express-validator": "^3.0.0",
    "@types/gravatar": "^1.8.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.30",
    "@types/node": "^14.0.19",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/request": "^2.48.5",
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  }
}

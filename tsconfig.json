{
  "compilerOptions": {
    "target": "es5",                          
    "module": "commonjs",                     
    "outFile": "./dist",                       
    "outDir": "./src",
    "typeRoots": ["./types"],                        
    "strict": true,                          
    "esModuleInterop": true,                  
    "skipLibCheck": true,                     
    "forceConsistentCasingInFileNames": true,
  },
  "exclude": [
      "node_modules"
  ]
}
